def perform_batch_matching(user_group_1, user_group_2, model, batch_name=""):
    """ë°°ì¹˜ ë‹¨ìœ„ë¡œ ë§¤ì¹­ ë¶„ì„ ìˆ˜í–‰"""
    batch_matches = []

    print(f"ğŸ”„ {batch_name} ë§¤ì¹­ ë¶„ì„ ì¤‘... ({len(user_group_1)}ëª… Ã— {len(user_group_2)}ëª… = {len(user_group_1) * len(user_group_2)}ìŒ)")

    for i, user1 in enumerate(user_group_1):
        for j, user2 in enumerate(user_group_2):
            if user1['id'] == user2['id']:
                continue  # ìê¸° ìì‹ ê³¼ì˜ ë§¤ì¹­ ì œì™¸

            try:
                # AIì—ê²Œ í˜¸í™˜ì„± ë¶„ì„ ìš”ì²­ (ê°„ë‹¨ ë²„ì „)
                prompt = f"""
                {user1['name']}({user1['mbti']}) â†” {user2['name']}({user2['mbti']})

                MBTI ê¸°ë°˜ í˜¸í™˜ì„± ë¶„ì„:
                í˜¸í™˜ì„± ì ìˆ˜: [1-100ì ]
                ë§¤ì¹­ ì´ìœ : [ë§¤ìš° ê°„ë‹¨í•œ í…ìŠ¤íŠ¸ ì„¤ëª…]
                """

                response = model.generate_content(prompt, generation_config=genai.types.GenerationConfig(
                    temperature=0.7,
                    max_output_tokens=100,  # ë§¤ìš° ì§§ê²Œ ì œí•œí•˜ì—¬ ì†ë„ ìµœì í™”
                ))

                if response is None:
                    print(f"âš ï¸ AI ì‘ë‹µì´ None: {user1['name']} â†” {user2['name']}")
                    continue

                ai_result = response.text.strip() if response.text else ""

                # HTML ì—ëŸ¬ ì‘ë‹µ ì²´í¬ (JSON íŒŒì‹± ì˜¤ë¥˜ ë°©ì§€)
                if not ai_result or ai_result.startswith('<!DOCTYPE') or '<html' in ai_result.lower():
                    print(f"âš ï¸ HTML ì—ëŸ¬ ì‘ë‹µ ê°ì§€: {user1['name']} â†” {user2['name']}")
                    continue

                # ê°„ë‹¨í•œ MBTI í˜¸í™˜ì„± ê³„ì‚°
                mbti1 = user1['mbti']
                mbti2 = user2['mbti']

                # ê¸°ë³¸ í˜¸í™˜ì„± ì ìˆ˜ ê³„ì‚° (MBTIë§Œ)
                base_score = 50  # ê¸°ë³¸ê°’

                # ê°™ì€ ì°¨ì›ë³„ ì ìˆ˜ ê°€ì‚°
                if mbti1[0] == mbti2[0]: base_score += 15  # I/E
                if mbti1[1] == mbti2[1]: base_score += 15  # S/N
                if mbti1[2] == mbti2[2]: base_score += 10  # T/F
                if mbti1[3] == mbti2[3]: base_score += 10  # J/P

                compatibility_score = min(base_score, 100)  # ìµœëŒ€ 100ì 

                # AI ì‘ë‹µì—ì„œ ì ìˆ˜ ì¶”ì¶œ ì‹œë„ (ìˆìœ¼ë©´ ì‚¬ìš©)
                if ai_result and 'í˜¸í™˜ì„± ì ìˆ˜:' in ai_result:
                    try:
                            import re
                        score_match = re.search(r'í˜¸í™˜ì„± ì ìˆ˜:\s*(\d+)', ai_result)
                            if score_match:
                            ai_score = int(score_match.group(1))
                            if 1 <= ai_score <= 100:
                                compatibility_score = ai_score
                        except:
                            pass

                # ë§¤ìš° ê°„ë‹¨í•œ ë§¤ì¹­ ì´ìœ  ìƒì„±
                if compatibility_score >= 80:
                    matching_reason = f"{user1['name']}ë‹˜ê³¼ {user2['name']}ë‹˜ì€ ë§¤ìš° ì˜ ë§ì•„ìš”"
                elif compatibility_score >= 60:
                    matching_reason = f"{user1['name']}ë‹˜ê³¼ {user2['name']}ë‹˜ì€ ì˜ ë§ëŠ” í¸ì´ì—ìš”"
                                else:
                    matching_reason = f"{user1['name']}ë‹˜ê³¼ {user2['name']}ë‹˜ì€ ë³´í†µ í˜¸í™˜ì„±ì…ë‹ˆë‹¤"

                # AI ì‘ë‹µì—ì„œ ë§¤ì¹­ ì´ìœ  ì¶”ì¶œ ì‹œë„ (ìˆìœ¼ë©´ ì‚¬ìš©)
                if 'ë§¤ì¹­ ì´ìœ :' in ai_result:
                    try:
                        reason_part = ai_result.split('ë§¤ì¹­ ì´ìœ :', 1)[1].strip()
                        if reason_part and len(reason_part.strip()) > 3:
                            matching_reason = reason_part.strip()[:50]  # 50ìë¡œ ì œí•œ
                    except:
                        pass

                batch_matches.append({
                    'user1_id': user1['id'],
                    'user2_id': user2['id'],
                    'user1_name': user1['name'],
                    'user2_name': user2['name'],
                    'compatibility_score': compatibility_score,
                    'matching_reason': matching_reason
                })

                print(f"âœ… {batch_name} {i+1}-{j+1}: {user1['name']} â†” {user2['name']} (ì ìˆ˜: {compatibility_score})")

            except Exception as e:
                print(f"âŒ {batch_name} ë§¤ì¹­ ì‹¤íŒ¨ {user1['name']} â†” {user2['name']}: {str(e)[:50]}...")
                continue

    return batch_matches

