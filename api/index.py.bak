def get_cached_mbti_compatibility(mbti1, mbti2, model=None):
    """MBTI í˜¸í™˜ì„± ìºì‹œ ì¡°íšŒ, ì—†ìœ¼ë©´ AIë¡œ ê³„ì‚°í•´ì„œ ìºì‹œ"""
    # ìºì‹œ ë¡œë“œ
    cache = load_mbti_compatibility_cache()

    # ì •ê·œí™”ëœ í‚¤ ìƒì„± (í•­ìƒ ì•ŒíŒŒë²³ìˆœìœ¼ë¡œ)
    key_parts = sorted([mbti1.upper(), mbti2.upper()])
    cache_key = f"{key_parts[0]}_{key_parts[1]}"

    # ìºì‹œ í™•ì¸
    if cache_key in cache:
        return cache[cache_key]

    # ìºì‹œì— ì—†ìœ¼ë©´ AIë¡œ ê³„ì‚°
    if model:
        try:
            prompt = f"""
            ë‹¤ìŒ ë‘ MBTI ìœ í˜•ì˜ ì—°ì• /ì»¤í”Œ í˜¸í™˜ì„±ì„ ë¶„ì„í•´ì£¼ì„¸ìš”:

            MBTI 1: {mbti1}
            MBTI 2: {mbti2}

            í˜¸í™˜ì„± ì ìˆ˜ë§Œ ìˆ«ìë¡œ ì‘ë‹µí•´ì£¼ì„¸ìš” (1-100ì ):
            """

            response = model.generate_content(prompt, generation_config=genai.types.GenerationConfig(
                temperature=0.3,
                max_output_tokens=10,
            ))

            if response and response.text:
                # ìˆ«ìë§Œ ì¶”ì¶œ
                import re
                score_match = re.search(r'\d+', response.text.strip())
                if score_match:
                    score = int(score_match.group())
                    score = max(1, min(100, score))

                    # ìºì‹œì— ì €ì¥
                    cache[cache_key] = score
                    save_mbti_compatibility_cache(cache)

                    print(f"ğŸ’¾ MBTI í˜¸í™˜ì„± ìºì‹œ ì €ì¥: {mbti1} â†” {mbti2} = {score}")
                    return score

        except Exception as e:
            print(f"âŒ MBTI í˜¸í™˜ì„± ê³„ì‚° ì‹¤íŒ¨: {e}")

    # AI ì‹¤íŒ¨ ì‹œ ê¸°ë³¸ ë£° ê¸°ë°˜ ê³„ì‚°
    base_score = 50  # ê¸°ë³¸ê°’

    # ê°™ì€ ì°¨ì›ë³„ ì ìˆ˜ ê°€ì‚°
    if mbti1[0] == mbti2[0]: base_score += 15  # I/E
    if mbti1[1] == mbti2[1]: base_score += 15  # S/N
    if mbti1[2] == mbti2[2]: base_score += 10  # T/F
    if mbti1[3] == mbti2[3]: base_score += 10  # J/P

    final_score = min(base_score, 100)

    # ë£° ê¸°ë°˜ ê³„ì‚°ë„ ìºì‹œì— ì €ì¥
    cache[cache_key] = final_score
    save_mbti_compatibility_cache(cache)

    print(f"ğŸ“Š ë£° ê¸°ë°˜ MBTI í˜¸í™˜ì„±: {mbti1} â†” {mbti2} = {final_score}")
    return final_score

def calculate_saju_compatibility(saju1, saju2):
    """ì‚¬ì£¼ í˜¸í™˜ì„± ë£° ê¸°ë°˜ ê³„ì‚° (ê¸°ìš´ ê¶í•©)"""
    try:
        # ì‚¬ì£¼ ë¬¸ìì—´ íŒŒì‹± (í˜•ì‹: ì—°ì£¼/ì›”ì£¼/ì¼ì£¼/ì‹œì£¼)
        parts1 = saju1.split('/')
        parts2 = saju2.split('/')

        if len(parts1) < 4 or len(parts2) < 4:
            return 25  # ê¸°ë³¸ê°’

        year1, month1, day1, hour1 = parts1[:4]
        year2, month2, day2, hour2 = parts2[:4]

        compatibility_score = 25  # ê¸°ë³¸ í˜¸í™˜ì„±

        # 1. ì—°ì£¼ ê¶í•© (ê°€ì¥ ì¤‘ìš”)
        year_compatibility = get_element_compatibility(year1, year2)
        compatibility_score += year_compatibility * 0.4  # 40% ê°€ì¤‘ì¹˜

        # 2. ì›”ì£¼ ê¶í•©
        month_compatibility = get_element_compatibility(month1, month2)
        compatibility_score += month_compatibility * 0.3  # 30% ê°€ì¤‘ì¹˜

        # 3. ì¼ì£¼ ê¶í•© (ìƒë…„ì›”ì¼ì˜ ì¼ì£¼)
        day_compatibility = get_element_compatibility(day1, day2)
        compatibility_score += day_compatibility * 0.2  # 20% ê°€ì¤‘ì¹˜

        # 4. ì‹œì£¼ ê¶í•© (ì¶œìƒì‹œê°„)
        hour_compatibility = get_element_compatibility(hour1, hour2)
        compatibility_score += hour_compatibility * 0.1  # 10% ê°€ì¤‘ì¹˜

        # ë²”ìœ„ ì œí•œ
        final_score = max(1, min(50, int(compatibility_score)))

        print(f"âš¡ ì‚¬ì£¼ í˜¸í™˜ì„± ê³„ì‚°: {saju1} â†” {saju2} = {final_score}/50")
        return final_score

    except Exception as e:
        print(f"âŒ ì‚¬ì£¼ í˜¸í™˜ì„± ê³„ì‚° ì˜¤ë¥˜: {e}")
        return 25  # ê¸°ë³¸ê°’

def get_element_compatibility(element1, element2):
    """ì˜¤í–‰ ê¸°ìš´ ê°„ í˜¸í™˜ì„± ê³„ì‚°"""
    # ì˜¤í–‰: ëª©(æœ¨), í™”(ç«), í† (åœŸ), ê¸ˆ(é‡‘), ìˆ˜(æ°´)
    # ê¶í•©í‘œ (1-10ì ): 10=ìµœìƒ, 1=ìµœì•…

    compatibility_matrix = {
        # ëª©(æœ¨)ê³¼ì˜ ê¶í•©
        'ëª©': {'ëª©': 7, 'í™”': 9, 'í† ': 3, 'ê¸ˆ': 1, 'ìˆ˜': 8},
        # í™”(ç«)ê³¼ì˜ ê¶í•©
        'í™”': {'ëª©': 8, 'í™”': 6, 'í† ': 10, 'ê¸ˆ': 5, 'ìˆ˜': 2},
        # í† (åœŸ)ê³¼ì˜ ê¶í•©
        'í† ': {'ëª©': 10, 'í™”': 5, 'í† ': 7, 'ê¸ˆ': 8, 'ìˆ˜': 3},
        # ê¸ˆ(é‡‘)ê³¼ì˜ ê¶í•©
        'ê¸ˆ': {'ëª©': 2, 'í™”': 8, 'í† ': 9, 'ê¸ˆ': 6, 'ìˆ˜': 10},
        # ìˆ˜(æ°´)ê³¼ì˜ ê¶í•©
        'ìˆ˜': {'ëª©': 5, 'í™”': 10, 'í† ': 8, 'ê¸ˆ': 3, 'ìˆ˜': 7}
    }

    # ê°„ì§€ì—ì„œ ì˜¤í–‰ ì¶”ì¶œ (ê°„ë‹¨í•œ ë§¤í•‘)
    element_map = {
        # ëª©(æœ¨): ê°‘ëª©, ì„ëª©, ì¸ëª©, ë¬˜ëª©
        'ê°‘': 'ëª©', 'ì„': 'ëª©', 'ì¸': 'ëª©', 'ë¬˜': 'ëª©',
        # í™”(ç«): ë³‘í™”, ì •í™”, ê³„í™”, ì„í™”
        'ë³‘': 'í™”', 'ì •': 'í™”', 'ê³„': 'í™”', 'ì„': 'í™”',
        # í† (åœŸ): ë¬´í† , ê¸°í† , ì‹ í† , ê²½í† 
        'ë¬´': 'í† ', 'ê¸°': 'í† ', 'ì‹ ': 'í† ', 'ê²½': 'í† ',
        # ê¸ˆ(é‡‘):åºšé‡‘, è¾›é‡‘, ç”³é‡‘, é…‰é‡‘
        'ê²½': 'ê¸ˆ', 'ì‹ ': 'ê¸ˆ', 'ç”³': 'ê¸ˆ', 'é…‰': 'ê¸ˆ',
        # ìˆ˜(æ°´):å£¬æ°´, ç™¸æ°´, å­æ°´, äº¥æ°´
        'ì„': 'ìˆ˜', 'ê³„': 'ìˆ˜', 'ì': 'ìˆ˜', 'í•´': 'ìˆ˜'
    }

    try:
        # ê°„ì§€ ì²« ê¸€ìë¡œ ì˜¤í–‰ ì¶”ì¶œ
        elem1 = element_map.get(element1[0] if element1 else '', 'í† ')  # ê¸°ë³¸ê°’ í† 
        elem2 = element_map.get(element2[0] if element2 else '', 'í† ')  # ê¸°ë³¸ê°’ í† 

        score = compatibility_matrix.get(elem1, {}).get(elem2, 5)  # ê¸°ë³¸ê°’ 5

        return score

    except Exception as e:
        print(f"âŒ ì˜¤í–‰ í˜¸í™˜ì„± ê³„ì‚° ì˜¤ë¥˜: {e}")
        return 5  # ê¸°ë³¸ê°’

def should_skip_ai_analysis(user1, user2, model=None):
    """AI ë¶„ì„ì„ ìƒëµí• ì§€ ê²°ì •í•˜ëŠ” ì‚¬ì „ í•„í„°ë§"""
    try:
        # 1. MBTI í˜¸í™˜ì„± í™•ì¸
        mbti_score = get_cached_mbti_compatibility(user1['mbti'], user2['mbti'], model)

        # 2. ì‚¬ì£¼ í˜¸í™˜ì„± í™•ì¸
        saju_score = calculate_saju_compatibility(user1['saju_result'], user2['saju_result'])

        # 3. í•„í„°ë§ ì¡°ê±´
        # MBTI í˜¸í™˜ì„±ì´ 60ì  ë¯¸ë§Œì´ê±°ë‚˜ ì‚¬ì£¼ í˜¸í™˜ì„±ì´ 30ì  ë¯¸ë§Œì¸ ê²½ìš° AI ìƒëµ
        if mbti_score < 60 or saju_score < 30:
            final_score = int((mbti_score * 0.6) + (saju_score * 1.2))  # ì‚¬ì£¼ ê°€ì¤‘ì¹˜ ë†’ì„
            final_score = max(20, min(69, final_score))  # 70ì  ë¯¸ë§Œ ë³´ì¥

            print(f"ğŸš« AI ë¶„ì„ ìƒëµ: {user1['name']}({user1['mbti']}) â†” {user2['name']}({user2['mbti']})")
            print(f"   MBTI: {mbti_score}, ì‚¬ì£¼: {saju_score} â†’ ìµœì¢…: {final_score}")

            return True, final_score, f"{user1['name']}ë‹˜ê³¼ {user2['name']}ë‹˜ì€ ê¸°ë³¸ì ì¸ MBTIÂ·ì‚¬ì£¼ í˜¸í™˜ì„±ì´ ë‚®ì•„ ê¹Šì´ ìˆëŠ” ë¶„ì„ì´ í•„ìš”í•˜ì§€ ì•Šì€ ê´€ê³„ì…ë‹ˆë‹¤."

        # í˜¸í™˜ì„±ì´ ë†’ìœ¼ë©´ AI ë¶„ì„ ì§„í–‰
        print(f"ğŸ¤– AI ë¶„ì„ ì§„í–‰: {user1['name']}({user1['mbti']}) â†” {user2['name']}({user2['mbti']})")
        print(f"   MBTI: {mbti_score}, ì‚¬ì£¼: {saju_score} â†’ ì ì¬ë ¥ ë†’ìŒ")

        return False, mbti_score, saju_score

    except Exception as e:
        print(f"âŒ ì‚¬ì „ í•„í„°ë§ ì˜¤ë¥˜: {e}")
        return False, 50, 25  # ê¸°ë³¸ê°’ìœ¼ë¡œ AI ë¶„ì„ ì§„í–‰

