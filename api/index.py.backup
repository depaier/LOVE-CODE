def perform_batch_matching(user_group_1, user_group_2, model, batch_name=""):
    """ë°°ì¹˜ ë‹¨ìœ„ë¡œ ë§¤ì¹­ ë¶„ì„ ìˆ˜í–‰"""
    batch_matches = []

    print(f"ğŸ”„ {batch_name} ë§¤ì¹­ ë¶„ì„ ì¤‘... ({len(user_group_1)}ëª… Ã— {len(user_group_2)}ëª… = {len(user_group_1) * len(user_group_2)}ìŒ)")

    for i, user1 in enumerate(user_group_1):
        for j, user2 in enumerate(user_group_2):
            if user1['id'] == user2['id']:
                continue  # ìê¸° ìì‹ ê³¼ì˜ ë§¤ì¹­ ì œì™¸

            try:
                # AIì—ê²Œ í˜¸í™˜ì„± ë¶„ì„ ìš”ì²­ (ê°„ë‹¨ ë²„ì „)
                prompt = f"""
                {user1['name']}({user1['mbti']}, ì‚¬ì£¼:{user1['saju_result']}) â†” {user2['name']}({user2['mbti']}, ì‚¬ì£¼:{user2['saju_result']})

                ì‚¬ì£¼ì™€ MBTI ê¸°ë°˜ í˜¸í™˜ì„± ë¶„ì„:
                ì‚¬ì£¼ í˜¸í™˜ì„±: [1-50ì ]
                MBTI í˜¸í™˜ì„±: [1-50ì ]
                ìµœì¢… í˜¸í™˜ì„± ì ìˆ˜: [1-100ì ]
                ë§¤ì¹­ ì´ìœ : [ê°„ë‹¨í•œ ì´ìœ  ì„¤ëª…]
                """

                response = model.generate_content(prompt, generation_config=genai.types.GenerationConfig(
                    temperature=0.7,
                    max_output_tokens=200,  # ë” ì§§ê²Œ ì œí•œ
                ))

                if response is None:
                    print(f"âš ï¸ AI ì‘ë‹µì´ None: {user1['name']} â†” {user2['name']}")
                    continue

                ai_result = response.text.strip() if response.text else ""

                # HTML ì—ëŸ¬ ì‘ë‹µ ì²´í¬ (JSON íŒŒì‹± ì˜¤ë¥˜ ë°©ì§€)
                if not ai_result or ai_result.startswith('<!DOCTYPE') or '<html' in ai_result.lower():
                    print(f"âš ï¸ HTML ì—ëŸ¬ ì‘ë‹µ ê°ì§€: {user1['name']} â†” {user2['name']}")
                    continue

                # ì ìˆ˜ íŒŒì‹± (ê°„ì†Œí™”) - MBTI ê¸°ë°˜ ê¸°ë³¸ê°’ ì„¤ì •
                mbti1 = user1['mbti']
                mbti2 = user2['mbti']

                # MBTI í˜¸í™˜ì„± ê¸°ë³¸ê°’ ì„¤ì •
                if mbti1[0] == mbti2[0]:  # ê°™ì€ ì„±í–¥ (I/E, S/N, T/F, J/P)
                    mbti_score = 35
                elif mbti1[1] == mbti2[1] or mbti1[2] == mbti2[2] or mbti1[3] == mbti2[3]:
                    mbti_score = 25
                else:
                    mbti_score = 15

                saju_score = 25  # ì‚¬ì£¼ ê¸°ë³¸ê°’
                compatibility_score = saju_score + mbti_score

                lines = ai_result.split('\n')
                for line in lines:
                    line = line.strip()
                    if 'ì‚¬ì£¼ í˜¸í™˜ì„±:' in line:
                        try:
                            score_text = line.split('ì‚¬ì£¼ í˜¸í™˜ì„±:')[1].strip()
                            import re
                            score_match = re.search(r'\d+', score_text)
                            if score_match:
                                saju_score = min(50, max(1, int(score_match.group())))
                        except:
                            pass
                    elif 'MBTI í˜¸í™˜ì„±:' in line:
                        try:
                            score_text = line.split('MBTI í˜¸í™˜ì„±:')[1].strip()
                            import re
                            score_match = re.search(r'\d+', score_text)
                            if score_match:
                                mbti_score = min(50, max(1, int(score_match.group())))
                        except:
                            pass
                    elif 'ìµœì¢… í˜¸í™˜ì„± ì ìˆ˜:' in line:
                        try:
                            score_text = line.split('ìµœì¢… í˜¸í™˜ì„± ì ìˆ˜:')[1].strip()
                            import re
                            score_match = re.search(r'\d+', score_text)
                            if score_match:
                                calculated_score = min(100, max(1, int(score_match.group())))
                                manual_score = saju_score + mbti_score
                                if abs(calculated_score - manual_score) <= 10:
                                    compatibility_score = calculated_score
                                else:
                                    compatibility_score = manual_score
                        except:
                            compatibility_score = saju_score + mbti_score

                # ë§¤ì¹­ ì´ìœ  ì¶”ì¶œ (ê°„ë‹¨í•˜ê²Œ)
                matching_reason = f"{user1['name']}ë‹˜ê³¼ {user2['name']}ë‹˜ì€ {compatibility_score}ì  í˜¸í™˜ì„±ì…ë‹ˆë‹¤."
                if 'ë§¤ì¹­ ì´ìœ :' in ai_result:
                    try:
                        reason_part = ai_result.split('ë§¤ì¹­ ì´ìœ :', 1)[1].strip()
                        if reason_part and len(reason_part) > 5:
                            matching_reason = reason_part[:100]  # 100ìë¡œ ì œí•œ
                    except:
                        pass

                batch_matches.append({
                    'user1_id': user1['id'],
                    'user2_id': user2['id'],
                    'user1_name': user1['name'],
                    'user2_name': user2['name'],
                    'compatibility_score': compatibility_score,
                    'matching_reason': matching_reason
                })

                print(f"âœ… {batch_name} {i+1}-{j+1}: {user1['name']} â†” {user2['name']} (ì ìˆ˜: {compatibility_score})")

            except Exception as e:
                print(f"âŒ {batch_name} ë§¤ì¹­ ì‹¤íŒ¨ {user1['name']} â†” {user2['name']}: {str(e)[:50]}...")
                continue

    return batch_matches

