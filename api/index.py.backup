def send_push_notification(subscription_info, title, body, data=None):
    """í‘¸ì‹œ ì•Œë¦¼ ì „ì†¡ - Node.js web-push ë¼ì´ë¸ŒëŸ¬ë¦¬ ì‚¬ìš©"""
    try:
        import subprocess
        import json
        import os
        import tempfile

        endpoint = subscription_info.get('endpoint', '')
        p256dh = subscription_info.get('keys', {}).get('p256dh', '')
        auth = subscription_info.get('keys', {}).get('auth', '')

        if not all([endpoint, p256dh, auth]):
            print("âŒ êµ¬ë… ì •ë³´ê°€ ë¶ˆì™„ì „í•©ë‹ˆë‹¤")
            return False

        print(f"ğŸ“¤ í‘¸ì‹œ ì•Œë¦¼ ì „ì†¡ ì‹œë„: {endpoint[:50]}...")
        print(f"ğŸ“ ì œëª©: {title}")
        print(f"ğŸ“ ë‚´ìš©: {body}")

        # ì•ˆì „í•œ ë¬¸ìì—´ë¡œ ë³€í™˜ (ì‰˜ ì¸ì ì…˜ ë°©ì§€)
        safe_title = title.replace('"', '\\"').replace("'", "\\'")
        safe_body = body.replace('"', '\\"').replace("'", "\\'")

        # Node.js ìŠ¤í¬ë¦½íŠ¸ë¡œ í‘¸ì‹œ ì•Œë¦¼ ì „ì†¡
        script_content = f'''
const webpush = require("web-push");

// VAPID í‚¤ ì§ì ‘ ì„¤ì • (Pythonì—ì„œ ì „ë‹¬ë°›ìŒ)
const vapidEmail = "{VAPID_EMAIL}";
const vapidPublicKey = "{VAPID_PUBLIC_KEY}";
const vapidPrivateKey = "{VAPID_PRIVATE_KEY}";

// VAPID í‚¤ ì„¤ì •
webpush.setVapidDetails(
  vapidEmail.startsWith('mailto:') ? vapidEmail : `mailto:${{vapidEmail}}`,
  vapidPublicKey,
  vapidPrivateKey
);

const subscription = {{
  endpoint: "{endpoint}",
  keys: {{
    p256dh: "{p256dh}",
    auth: "{auth}"
  }}
}};

const payload = JSON.stringify({{
  title: "{safe_title}",
  body: "{safe_body}",
  icon: "{APP_URL}/static/img/kor.gif",
  badge: "{APP_URL}/static/img/kor.gif",
  data: {json.dumps(data) if data else '{}'}
}});

console.log("Node.jsì—ì„œ í‘¸ì‹œ ì•Œë¦¼ ì „ì†¡ ì‹œë„...");

webpush.sendNotification(subscription, payload)
  .then(result => {{
    console.log("âœ… í‘¸ì‹œ ì•Œë¦¼ ì „ì†¡ ì„±ê³µ");
    process.exit(0);
  }})
  .catch(err => {{
    console.error("âŒ í‘¸ì‹œ ì•Œë¦¼ ì „ì†¡ ì‹¤íŒ¨:", err.message);
    console.error("ì—ëŸ¬ ì½”ë“œ:", err.statusCode);
    process.exit(1);
  }});
'''

        # í”„ë¡œì íŠ¸ ë£¨íŠ¸ì— ì„ì‹œ íŒŒì¼ ìƒì„±
        project_root = os.path.dirname(os.path.dirname(os.path.abspath(__file__)))
        script_path = os.path.join(project_root, f'push_temp_{os.urandom(8).hex()}.js')

        try:
            # ì„ì‹œ íŒŒì¼ë¡œ Node.js ìŠ¤í¬ë¦½íŠ¸ ìƒì„± (í”„ë¡œì íŠ¸ ë£¨íŠ¸ì—)
            with open(script_path, 'w', encoding='utf-8') as f:
                f.write(script_content)

            # Node.js ìŠ¤í¬ë¦½íŠ¸ ì‹¤í–‰ (í”„ë¡œì íŠ¸ ë£¨íŠ¸ì—ì„œ)
            env = os.environ.copy()
            env.update({
                'VAPID_PRIVATE_KEY': VAPID_PRIVATE_KEY,
                'VAPID_PUBLIC_KEY': VAPID_PUBLIC_KEY,
                'VAPID_EMAIL': VAPID_EMAIL,
                'APP_URL': APP_URL
            })

            result = subprocess.run(
                ['node', os.path.basename(script_path)],
                capture_output=True,
                text=True,
                timeout=15,
                cwd=project_root,  # í”„ë¡œì íŠ¸ ë£¨íŠ¸ì—ì„œ ì‹¤í–‰
                env=env  # í™˜ê²½ë³€ìˆ˜ ì „ë‹¬
            )

            print("ğŸ“¡ Node.js ì¶œë ¥:")
            if result.stdout:
                print(result.stdout.strip())
            if result.stderr:
                print("ğŸ“¡ Node.js ì˜¤ë¥˜:")
                print(result.stderr.strip())

            return result.returncode == 0

        finally:
            # ì„ì‹œ íŒŒì¼ ì‚­ì œ
            try:
                os.unlink(script_path)
            except:
                pass

    except Exception as e:
        print(f"âŒ í‘¸ì‹œ ì•Œë¦¼ ì „ì†¡ ì‹¤íŒ¨: {e}")
        import traceback
        print(f"ìƒì„¸ ì˜¤ë¥˜: {traceback.format_exc()}")
        return False

def save_push_subscription(device_token, subscription_data, user_id=None):
    """í‘¸ì‹œ êµ¬ë… ì •ë³´ ì €ì¥"""
    try:
        supabase.table('push_subscriptions').upsert({
            'device_token': device_token,
            'endpoint': subscription_data['endpoint'],
            'p256dh': subscription_data['keys']['p256dh'],
            'auth': subscription_data['keys']['auth'],
            'user_id': user_id
        }).execute()
        return True
    except Exception as e:
        print(f"âŒ í‘¸ì‹œ êµ¬ë… ì €ì¥ ì‹¤íŒ¨: {e}")
        return False

